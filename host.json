{
    "version": "2.0",
    "extensionBundle": {
        "id": "Microsoft.Azure.Functions.ExtensionBundle",
        "version": "[4.*, 5.0.0)"
    },
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            }
        },
        "fileLoggingMode": "debugOnly",
        "logLevel": {
            "default": "Warning",
            "Host.Aggregator": "Trace",
            "Host.Results": "Information",
            "Function": "Information"
        }
    },
    "extensions": {
        "http": {
            "routePrefix": ""
        }
    }
}